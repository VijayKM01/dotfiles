*/5 * * * * /bin/rm -rf ~/.waterfox ~/.mozilla ~/.java ~/.macromedia ~/.adobe ~/downloads/mconnect
*/15 * * * * export DBUS_SESSION_BUS_ADDRESS=unix:path=/run/user/1000/bus; export DISPLAY=:0; . ~/.profile; ~/.local/bin/cron/feedup
*/5 * * * * export DBUS_SESSION_BUS_ADDRESS=unix:path=/run/user/1000/bus; export DISPLAY=:0; . ~/.profile; ~/.local/bin/cron/cronbat
*/3 * * * * /usr/local/bin/mailsync

